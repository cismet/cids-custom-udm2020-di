<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<!--
		called in: \gen\gen_cids\cids-distribution-uim2020-di-master -> ..\..\..\
	-->
    <localRepository>../../../lib/m2</localRepository>  
    <profiles>
        <profile>
            <id>global-properties</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
			
			<properties>
                <de.cismet.cidsDistDir>../../..</de.cismet.cidsDistDir>
                <!-- path to the navigator workdir in use, used to run the navigator from NB -->
                <de.cismet.cids.navigator.workingDir.uim2020-di>${de.cismet.cidsDistDir}/client/udm2020-di</de.cismet.cids.navigator.workingDir.uim2020-di>
                <!-- path to the server workdir in use, used to run the server from NB -->
                <de.cismet.cids.server.workingDir.uim2020-di>${de.cismet.cidsDistDir}/server/udm2020-di</de.cismet.cids.server.workingDir.uim2020-di>
                <de.cismet.cids.server.workingDir>${de.cismet.cidsDistDir}/server/udm2020-di</de.cismet.cids.server.workingDir>
                <!-- path to the local folder in use, required by the cids maven plugin -->
                <de.cismet.cids.lib.local>${de.cismet.cidsDistDir}/lib/localUdm2020-di</de.cismet.cids.lib.local>
                <de.cismet.cids.server.rest.workingDir.uim2020-di>${de.cismet.cidsDistDir}/server/udm2020-di-rest</de.cismet.cids.server.rest.workingDir.uim2020-di>
                <de.cismet.cids.server.rest.workingDir>${de.cismet.cidsDistDir}/server/udm2020-di-rest</de.cismet.cids.server.rest.workingDir>
            </properties>
			
            <repositories>
                <repository>
                    <id>eig-cismet-repo</id>
                    <url>https://repo.cismet.de/repo</url>
                    <releases>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cismet-repo</id>
                    <url>https://repo.cismet.de/repo</url>
                    <releases>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>cismet-dev</id>
                    <url>https://repo.cismet.de/repo</url>
                    <releases>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>eig-cismet-repo</id>
                    <url>https://repo.cismet.de/repo</url>
                    <releases>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
				<pluginRepository>
                    <id>cismet-private-repo</id>
                    <name>plugins-releases-local</name>
                    <url>https://repo.cismet.de/plugins-releases-local</url>
                    <releases>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    <mirrors>
        <mirror>
            <id>central-proxy</id>
            <name>cismet-repo</name>
            <url>https://repo.cismet.de/repo</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>cismet-dev</id>
            <username>****</username>
            <password>****</password>
        </server>
        <server>
            <id>eig-cismet-repo</id>
            <username>****</username>
            <password>****</password>
        </server>
        <server>
            <id>cismet-repo</id>
            <username>****</username>
            <password>****</password>
        </server>
        <server>
            <id>central</id>
            <username>****</username>
            <password>****</password>
        </server>
        <server>
            <id>central-proxy</id>
            <username>****</username>
            <password>****</password>
        </server>
    </servers>
</settings>
