            <Keyword>GEOIMAGE</Keyword>
            <Keyword>ORTHOPHOTO</Keyword>
            <Keyword>ORTHOFOTO</Keyword>
            <Keyword>REMOTE SENSING</Keyword>
            <Keyword>AUSTRIA</Keyword>
            <Keyword>LFRZ</Keyword>
