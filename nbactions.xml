<?xml version="1.0" encoding="UTF-8"?>
<actions>
    <action>
        <actionName>build</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>install</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>clean</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>clean</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>run.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
            <jpda.listen>true</jpda.listen>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug.test.single</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test-compile</goal>
            <goal>surefire:test</goal>
        </goals>
        <properties>
            <test>${packageClassName}</test>
            <forkMode>once</forkMode>
            <maven.surefire.debug>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address}</maven.surefire.debug>
            <jpda.listen>true</jpda.listen>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>profile.single.main</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.args>-classpath %classpath ${packageClassName}</exec.args>
            <exec.executable>java</exec.executable>
            <exec.classpathScope>${classPathScope}</exec.classpathScope>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>javadoc</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>generate-sources</goal>
            <goal>javadoc:javadoc</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>run</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.workingdir>${de.cismet.cids.navigator.workingDir.uim2020-di}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Duser.language=en -Duser.country=GB -Duser.variant=uim2020-di -Ddirectory.extension=uim2020-di.netbeans -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/log4j.debug.properties ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/navigator.cfg ${de.cismet.cids.navigator.workingDir.uim2020-di} ${de.cismet.cids.navigator.workingDir.uim2020-di}/plugins ${de.cismet.cids.navigator.workingDir.uim2020-di}/search ${de.cismet.cids.navigator.workingDir.uim2020-di}/profiles</exec.args>
            <exec.executable>java</exec.executable>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
            <activatedProfile>switch-on</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>debug</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.workingdir>${de.cismet.cids.navigator.workingDir.uim2020-di}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>-Duser.language=en -Duser.country=GB -Duser.variant=uim2020-di -Ddirectory.extension=uim2020-di.netbeans -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/log4j.debug.properties ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/navigator.cfg ${de.cismet.cids.navigator.workingDir.uim2020-di} ${de.cismet.cids.navigator.workingDir.uim2020-di}/plugins ${de.cismet.cids.navigator.workingDir.uim2020-di}/search ${de.cismet.cids.navigator.workingDir.uim2020-di}/profiles</exec.args>
            <exec.executable>java</exec.executable>
            <jpda.listen>true</jpda.listen>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
            <activatedProfile>switch-on</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>profile</actionName>
        <packagings>
            <packaging>jar</packaging>
        </packagings>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:exec</goal>
        </goals>
        <properties>
            <exec.workingdir>${de.cismet.cids.navigator.workingDir.uim2020-di}</exec.workingdir>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.args>${profiler.args} -Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Duser.language=en -Duser.country=GB -Duser.variant=uim2020-di -Ddirectory.extension=uim2020-di.netbeans -Xms256m -Xmx2g -classpath ${de.cismet.cids.classpath} Sirius.navigator.Navigator ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/log4j.debug.properties ${de.cismet.cids.navigator.workingDir.uim2020-di}/config/navigator.cfg ${de.cismet.cids.navigator.workingDir.uim2020-di} ${de.cismet.cids.navigator.workingDir.uim2020-di}/plugins ${de.cismet.cids.navigator.workingDir.uim2020-di}/search ${de.cismet.cids.navigator.workingDir.uim2020-di}/profiles</exec.args>
            <exec.executable>${profiler.java}</exec.executable>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild</activatedProfile>
            <activatedProfile>cismet-dev-exec</activatedProfile>
            <activatedProfile>switch-on</activatedProfile>
        </activatedProfiles>
    </action>
    <action>
        <actionName>CUSTOM-debug.test</actionName>
        <packagings>
            <packaging>*</packaging>
        </packagings>
        <goals>
            <goal>test</goal>
        </goals>
        <properties>
            <forkMode>once</forkMode>
            <maven.surefire.debug>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address}</maven.surefire.debug>
            <jpda.listen>true</jpda.listen>
        </properties>
        <activatedProfiles>
            <activatedProfile>netbeans-fastbuild-test</activatedProfile>
        </activatedProfiles>
    </action>
</actions>
